# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Cloud Run Configuration
CLOUD_RUN_SERVICE_URL=https://your-service-name-xxxxxxxxxx-uc.a.run.app
CLOUD_RUN_SERVICE_TOKEN=your_cloud_run_service_token
CLOUD_RUN_WEBHOOK_SECRET=your_webhook_secret

# Google AI Configuration (for Cloud Run service)
GOOGLE_GEMINI_API_KEY=your_gemini_api_key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_VERSION=1.0.0
NODE_ENV=development

# Service Authentication
SERVICE_SECRET_TOKEN=your_service_secret_token

# Logging Configuration
ENABLE_SUPABASE_LOGGING=false

# Optional: Email Configuration
EMAIL_FROM=noreply@yourdomain.com
RESEND_API_KEY=your_resend_api_key